{
  "name": "md-blog",
  "private": true,
  "description": "A simplified blog site developed using GATSBYjs to render .md files and others (CMS,..)",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.3",
    "@contentful/rich-text-types": "^14.1.2",
    "dotenv": "^8.6.0",
    "gatsby": "^3.6.2",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-layout": "^2.6.0",
    "gatsby-plugin-mdx": "^2.6.0",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-sass": "^4.7.0",
    "gatsby-plugin-sharp": "^3.7.0",
    "gatsby-remark-images": "^5.4.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-contentful": "^5.7.1",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-json": "^3.7.1",
    "gatsby-transformer-remark": "^4.4.0",
    "gatsby-transformer-sharp": "^3.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.34.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "gh-pages": "^3.2.2",
    "prettier": "2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wacky199/md-blog.git"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
